{
    "expo": {
        "name": "ELSO Educational Tools Application",
        "slug": "ELSO Educational Tools Application",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "light",
        "splash": {
            "image": "./assets/elso-splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "icon": "./assets/icon.png",
            "bundleIdentifier": "org.elso.educational-tools",
            "supportsTablet": true,
            "requireFullScreen": true
        }
    }
}
